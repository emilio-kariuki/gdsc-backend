
{
  "compilerOptions": {
    "sourceMap": true,
    "outDir": "./dist/",
    "rootDir": "./src/",
    "strict": true,
    "lib": ["ES2016", "ESNext.AsyncIterable"],
    "esModuleInterop": true,
    "target": "ESNext",
    "noEmit" :false,
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "noFallthroughCasesInSwitch": true,

  },
  "include": ["./src/**/*.ts"],
  "exclude": ["node_modules"]
}
